{
  "name": "nextra-docs-template",
  "version": "0.0.1",
  "description": "Nextra docs template",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "postbuild": "pagefind --site .next/server/app --output-path public/_pagefind",
    "start": "next start",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "lint:staged": "biome lint --write --staged .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "format:staged": "biome format --write --staged .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "check:staged": "biome check --write --staged .",
    "check:ci": "biome ci .",
    "check:ci:github": "biome ci --reporter=github .",
    "check:ci:json": "biome ci --reporter=json .",
    "check:ci:junit": "biome ci --reporter=junit .",
    "check:changed": "biome check --write --changed .",
    "pre-commit": "biome check --write --staged .",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true next build",
    "verify": "./scripts/verify-production.sh",
    "turbo": "turbo",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:types": "drizzle-kit generate:types",
    "graphql:codegen": "graphql-codegen --config codegen.yml",
    "graphql:codegen:watch": "graphql-codegen --config codegen.yml --watch",
    "types:generate": "run-s db:types graphql:codegen",
    "types:watch": "run-s db:types graphql:codegen:watch",
    "generate:api-docs": "tsx scripts/generate-api-docs.ts",
    "generate:api-docs:watch": "tsx --watch scripts/generate-api-docs.ts",
    "generate:meta": "tsx scripts/generate-meta-with-deps.ts",
    "generate:meta:watch": "tsx scripts/generate-meta-with-deps.ts --watch",
    "api:generate": "run-s generate:api-docs types:generate",
    "docs:generate": "run-s generate:meta generate:api-docs",
    "validate:zod": "tsx scripts/validate-zod-schemas.ts",
    "migrate:zod-imports": "tsx scripts/migrate-zod-imports.ts",
    "zod:validate": "run-s check validate:zod",
    "zod:migrate": "migrate:zod-imports",
    "organize-docs": "tsx scripts/organize-docs.ts",
    "organize-docs:dry-run": "tsx scripts/organize-docs.ts --dry-run",
    "validate-docs": "tsx scripts/validate-docs.ts",
    "docs:lint": "markdownlint-cli2 \"docs/**/*.md\" \".cursor/docs/**/*.md\"",
    "docs:lint:fix": "markdownlint-cli2 --fix \"docs/**/*.md\" \".cursor/docs/**/*.md\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shuding/nextra-docs-template.git"
  },
  "author": "Shu Ding <g@shud.in>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shuding/nextra-docs-template/issues"
  },
  "homepage": "https://github.com/shuding/nextra-docs-template#readme",
  "engines": {
    "node": ">=20.9.0 <23.0.0",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "@apollo/client": "^4.0.11",
    "@apollo/server": "^5.2.0",
    "@as-integrations/next": "^4.1.0",
    "@asteasolutions/zod-to-openapi": "^4.0.0",
    "@graphql-codegen/cli": "^6.1.0",
    "@graphql-codegen/typescript": "^5.0.7",
    "@graphql-codegen/typescript-operations": "^5.0.7",
    "@graphql-codegen/typescript-react-apollo": "^4.4.0",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-query": "^5.90.16",
    "@trpc/client": "^11.8.1",
    "@trpc/next": "^11.8.1",
    "@trpc/react-query": "^11.8.1",
    "@trpc/server": "^11.8.1",
    "@vercel/analytics": "^1.3.1",
    "@vercel/speed-insights": "^1.1.0",
    "chokidar": "^4.0.3",
    "clsx": "^2.1.1",
    "codegen": "^0.1.0",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "fast-glob": "^3.3.3",
    "graphql": "^16.12.0",
    "graphql-tag": "^2.12.6",
    "gray-matter": "^4.0.3",
    "katex": "^0.16.11",
    "mermaid": "^11.4.0",
    "next": "^16.1.1",
    "next-mdx-remote": "^5.0.0",
    "nextra": "latest",
    "nextra-theme-docs": "latest",
    "pino": "^10.1.1",
    "pino-pretty": "^13.1.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rehype-katex": "^7.0.1",
    "remark-math": "^6.0.0",
    "swagger-ui-react": "^5.17.14",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@next/bundle-analyzer": "^15.1.6",
    "@theguild/remark-mermaid": "0.1.0",
    "@theguild/remark-npm2yarn": "^0.3.0",
    "@types/katex": "^0.16.7",
    "@types/mdx": "^2.0.13",
    "@types/node": "^20.18.0",
    "@types/pg": "^8.16.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/swagger-ui-react": "^5.18.0",
    "drizzle-kit": "^0.31.8",
    "npm-run-all2": "^8.0.4",
    "pagefind": "^1.4.0",
    "postgres": "^3.4.8",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-mermaid": "^3.0.0",
    "rehype-prism-plus": "^1.0.6",
    "rehype-slug": "^6.0.0",
    "remark-callouts": "^2.0.0",
    "remark-gfm": "^4.0.0",
    "server-only": "^0.0.1",
    "tsx": "^4.19.2",
    "turbo": "^2.3.3",
    "typescript": "^5.7.2",
    "zod-to-json-schema": "^3.23.5",
    "chalk": "^5.3.0",
    "glob": "^11.0.0",
    "markdownlint-cli2": "^0.11.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.0"
  }
}
