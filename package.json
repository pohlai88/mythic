{
  "name": "@mythic/monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "AXIS Luxury Business Operating System - Turborepo Monorepo",
  "scripts": {
    "dev": "turbo run dev",
    "dev:docs": "turbo run dev --filter=@mythic/docs",
    "dev:boardroom": "turbo run dev --filter=@mythic/boardroom",
    "build": "turbo run build",
    "build:filter": "turbo run build --filter",
    "build:changed": "turbo run build --filter=[HEAD^1]",
    "start": "next start",
    "start:turbo": "turbo run start",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "lint:changed": "turbo run lint --filter=[HEAD^1]",
    "type-check": "turbo run type-check",
    "type-check:changed": "turbo run type-check --filter=[HEAD^1]",
    "format": "turbo run format",
    "format:check": "turbo run format:check",
    "check": "turbo run check",
    "check:fix": "turbo run check:fix",
    "check:changed": "turbo run check --filter=[HEAD^1]",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "test:ui": "turbo run test:ui",
    "test:coverage": "turbo run test:coverage",
    "test:changed": "turbo run test --filter=[HEAD^1]",
    "verify": "turbo run verify",
    "verify:changed": "turbo run verify --filter=[HEAD^1]",
    "clean": "turbo run clean && rm -rf node_modules .turbo",
    "clean:cache": "turbo run clean --force",
    "analyze": "ANALYZE=true turbo run analyze",
    "analyze:compliance": "tsx scripts/analyze-monorepo-compliance.ts",
    "turbo:graph": "turbo run build --graph",
    "turbo:summary": "turbo run build --summarize",
    "turbo:dry-run": "turbo run build --dry-run",
    "turbo:cache:status": "turbo run build --summarize",
    "turbo:graph:html": "turbo run build --graph > task-graph.html",
    "turbo:build:changed": "turbo run build --filter=[HEAD^1]",
    "turbo:build:deps": "turbo run build --filter=...@mythic/shared-utils",
    "turbo:build:all": "turbo run build --filter=@mythic/docs^... --filter=@mythic/boardroom^...",
    "validate:rules": "tsx scripts/validate-rule-config.ts",
    "validate:rules:fix": "tsx scripts/validate-rule-config.ts --fix",
    "validate:rules:strict": "tsx scripts/validate-rule-config.ts --strict",
    "audit:docs": "tsx scripts/audit-documentation.ts",
    "audit:docs:registry": "tsx scripts/audit-documentation.ts --registry",
    "audit:docs:cleanliness": "tsx scripts/audit-documentation-cleanliness.ts",
    "audit:docs:full": "tsx scripts/audit-documentation-cleanliness.ts",
    "verify:remediation": "tsx scripts/verify-remediation.ts",
    "fix:docs:naming": "tsx scripts/fix-docs-naming.ts --fix",
    "validate:docs:naming": "tsx scripts/validate-docs-naming.ts",
    "validate:docs:naming:fix": "tsx scripts/validate-docs-naming.ts --fix",
    "validate:docs:deps": "tsx scripts/validate-docs-dependencies.ts",
    "generate:api-docs": "tsx scripts/generate-api-docs.ts",
    "generate:type-docs": "tsx scripts/generate-type-docs.ts",
    "generate:component-docs": "tsx scripts/generate-component-docs.ts",
    "generate:function-docs": "tsx scripts/generate-function-docs.ts",
    "generate:docs": "tsx scripts/generate-all-docs.ts",
    "generate:docs:watch": "tsx scripts/generate-docs-watch.ts",
    "validate:verbatim": "tsx scripts/validate-verbatim.ts",
    "validate:tech-debt": "tsx scripts/validate-tech-debt.ts",
    "validate:imports": "tsx scripts/validate-imports.ts",
    "validate:design-system-sealed": "tsx scripts/validate-design-system-sealed.ts",
    "validate:css-files": "tsx scripts/validate-css-files.ts",
    "validate:zod-usage": "tsx scripts/validate-zod-usage.ts",
    "validate:tokens": "tsx scripts/validate-handoff-tokens.ts",
    "validate:figma-token": "tsx scripts/validate-figma-token.ts",
    "tokens:sync": "cd packages/design-system && pnpm tokens:sync",
    "tokens:sync-mcp": "tsx scripts/sync-figma-tokens-mcp.ts",
    "tokens:sync-to-figma": "tsx scripts/sync-tokens-to-figma.ts",
    "tokens:validate": "tsx scripts/validate-handoff-tokens.ts",
    "tokens:update-css": "tsx scripts/update-css-from-handoff.ts",
    "tokens:rebuild": "cd packages/design-system && pnpm build:css",
    "tokens:full-sync": "pnpm tokens:sync && pnpm tokens:validate && pnpm tokens:update-css && pnpm tokens:rebuild",
    "tokens:full-sync-mcp": "pnpm tokens:sync-mcp && pnpm tokens:validate && pnpm tokens:update-css && pnpm tokens:rebuild",
    "figma:export-components": "tsx scripts/figma-export-components.ts",
    "figma:export-styles": "FIGMA_FILE_KEY=${FIGMA_FILE_KEY} figma-export styles",
    "figma:export-icons": "FIGMA_FILE_KEY=${FIGMA_FILE_KEY} figma-export icons",
    "figma:code-connect": "tsx scripts/figma-code-connect.ts",
    "figma:list-files": "tsx scripts/figma-list-files.ts",
    "measure:server-components": "tsx scripts/measure-server-components.ts",
    "elite:gate": "tsx scripts/elite-gate.ts",
    "validate:all": "pnpm validate:verbatim && pnpm validate:rules && pnpm validate:docs:naming && pnpm validate:tech-debt && pnpm validate:zod-usage",
    "audit:360": "tsx scripts/audit-360.ts",
    "audit:tech-debt": "tsx scripts/audit-tech-debt.ts",
    "compute:doc-hash": "tsx scripts/compute-doc-hash.ts",
    "db:setup": "tsx scripts/setup-database.ts",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:performance": "cd apps/boardroom && pnpm test:db-performance",
    "validate:db": "cd apps/boardroom && pnpm validate:db",
    "docker:up": "cd apps/boardroom && pnpm docker:up",
    "docker:down": "cd apps/boardroom && pnpm docker:down",
    "docker:logs": "cd apps/boardroom && pnpm docker:logs",
    "schema:generate": "drizzle-kit generate && tsx scripts/generate-living-schema.ts",
    "tailwind:build": "tailwindcss -i ./apps/styles/globals.css -o ./dist/tailwind.css --minify",
    "tailwind:watch": "tailwindcss -i ./apps/styles/globals.css -o ./dist/tailwind.css --watch",
    "tailwind:boardroom": "cd apps/boardroom && tailwindcss -i ./styles/globals.css -o ./dist/tailwind.css --watch",
    "tailwind:docs": "cd apps/docs && tailwindcss -i ./styles/globals.css -o ./dist/tailwind.css --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mythic/monorepo.git"
  },
  "author": "Mythic Team",
  "license": "MIT",
