{
  "name": "@mythic/straton-hub",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "pnpm search:build-index && next build",
    "build:analyze": "cross-env ANALYZE=true next build --webpack",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,mdx}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,mdx}\"",
    "docs:validate": "tsx scripts/validate-docs.ts",
    "docs:validate:diataxis": "tsx scripts/validate-diataxis-structure.ts",
    "validate:templates": "tsx scripts/validate-templates.ts",
    "cleanup:diataxis": "tsx scripts/cleanup-diataxis-violations.ts",
    "docs:check-pollution": "tsx scripts/check-pollution.ts",
    "docs:check-nextjs": "tsx scripts/check-nextjs-compliance.ts",
    "docs:check-tailwind": "tsx scripts/check-tailwind-compliance.ts",
    "validate:all": "pnpm docs:validate && pnpm docs:validate:diataxis && pnpm docs:check-nextjs && pnpm docs:check-tailwind",
    "readme:validate-all": "tsx scripts/validate-readme-schema.ts",
    "readme:check-only": "tsx scripts/check-readme-only.ts",
    "search:build-index": "tsx scripts/build-search-index.ts",
    "check:silence-budget": "tsx scripts/validate-silence-budget.ts",
    "check:ux-regression": "tsx scripts/check-ux-regression.ts",
    "check:governance": "tsx scripts/validate-silence-budget.ts && tsx scripts/check-ux-regression.ts",
    "optimize:check": "pnpm lint && pnpm type-check && pnpm format:check"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.0.0",
    "@mdx-js/react": "^3.0.0",
    "@mythic/tailwindcss-v4-design-system": "workspace:*",
    "@mythic/nextjs-shared-utils": "workspace:*",
    "@next/mdx": "^16.1.1",
    "@tanstack/react-query": "^5.56.0",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "cmdk": "^1.0.0",
    "drizzle-zod": "^0.5.0",
    "fuse.js": "^7.0.0",
    "katex": "^0.16.27",
    "next": "^16.1.1",
    "next-themes": "^0.4.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "yaml": "^2.8.2",
    "zod": "^4.3.5",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.1.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/react-query-devtools": "^5.56.0",
    "@types/node": "^20.11.0",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "glob": "^11.0.0",
    "postcss": "^8.4.47",
    "remark-frontmatter": "^5.0.0",
    "swagger-ui-react": "^5.31.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
